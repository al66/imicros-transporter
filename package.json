{
  "name": "imicros-transporter",
  "version": "0.0.1",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Kafka transporter for Moleculer services",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache --config=travis.js --silent",
    "test-local": "jest --coverage --no-cache --config=local.js --silent"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/",
      "/test/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/",
      "/test/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-transporter.git"
  },
  "keywords": [
    "moleculer",
    "transporter",
    "kafka"
  ],
  "devDependencies": {
    "benchmarkify": "^2.1.2",
    "bluebird": "^3.7.2",
    "cote": "^1.0.0",
    "kafka-node": "^5.0.0",
    "nanoservices": "0.0.11",
    "nats": "^1.4.9",
    "nats-hemera": "^7.0.2",
    "seneca": "^3.21.2",
    "seneca-nats-transport": "^2.0.3",
    "wtfnode": "^0.8.1"
  },
  "dependencies": {
    "kafkajs": "^1.12.0",
    "moleculer": "^0.14.7",
    "uuid": "^8.2.0"
  }
}
